<div class="w-1/2 h-full flex items-end justify-end sm:pr-40">
  
      
      <ul class="flex flex-col items-center">
        <h2 class="font-bold py-5">Event Categories</h2>
        <% EventTypes.forEach(eventType => { %>
        <button
          class="mb-4 w-48 px-6 py-3 text-gray-800 font-semibold rounded-lg shadow-md hover:bg-gray-100 hover:shadow-lg focus:outline-none focus:ring-2 focus:ring-gray-300 focus:ring-opacity-50 transition duration-300 ease-in-out transform hover:scale-105 eventType"
          data-type="<%= eventType.name %>"
        >
          <%= eventType.name %>
        </button>
        <% }); %>
      </ul>
    </div>

